{
  "name": "techart/image-service",
  "version": "1.0.2",
  "description": "Provider to libraries for working with images",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Techart",
      "homepage": "https://web-techart.ru"
    }
  ],
  "require": {
    "php": "^8.2",
    "intervention/image": "^3.2",
    "gumlet/php-image-resize": "^2.0",
    "illuminate/support": ">=8",
    "illuminate/contracts": ">=8"
  },
  "autoload": {
    "psr-4": {
      "Techart\\ImageService\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10",
    "symfony/var-dumper": "^7.0",
    "ext-exif": "*"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit tests",
    "test-coverage": "XDEBUG_MODE=coverage ./vendor/bin/phpunit --testdox --display-warnings --display-deprecations tests"
  }
}
